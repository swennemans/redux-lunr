{
  "name": "react-tdd-tape",
  "version": "0.0.1",
  "description": "Testing React with Tape",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "watch": {
    "test-dev": "*.test.js"
  },
  "scripts": {
    "start": "node devServer.js",
    "test": "NODE_ENV=test karma start",
    "test-dev": "NODE_ENV=test babel-node ./test/*.js | faucet",
    "watch": "npm-watch",
    "watch-test": "npm run watch -s"
  },
  "author": "Sven Roeterdink <sven.roeterdink1@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "lunr": "^0.5.12"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "faucet": "0.0.1",
    "tape": "^4.2.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.3.0",
    "worker-loader": "^0.6.0"
  },
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
