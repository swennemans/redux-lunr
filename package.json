    "redux-thunk": "^1.0.0",
    "worker-loader": "^0.6.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "faucet": "0.0.1",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-tap": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "npm-watch": "0.0.1",
