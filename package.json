{
  "name": "react-tdd-tape",
  "version": "0.0.1",
  "description": "Testing React with Tape",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "watch": {
    "test-dev": "*.test.js"
  },
  "scripts": {
    "start": "node devServer.js",
